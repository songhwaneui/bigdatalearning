tial([\n","    keras.layers.Reshape((28,28,1)),\n","    \n","    keras.layers.Conv2D(16,3,padding=\"same\",activation=tf.nn.relu),\n","    keras.layers.Dropout(rate=0.2),\n","    keras.layers.MaxPool2D(pool_size=2),\n","    \n","    \n","    keras.layers.Conv2D(32,3,padding=\"same\",activation=tf.nn.relu),\n","    keras.layers.Dropout(rate=0.2),\n","    keras.layers.MaxPool2D(pool_size=2),\n","    \n","    \n","    keras.layers.Conv2D(64,3,padding=\"same\",activation=tf.nn.relu),\n","    keras.layers.Dropout(rate=0.2),\n","    keras.layers.Flatten(input_shape = (7,7)),\n","    \n","    keras.layers.Dense(512, activation=tf.nn.relu),\n","    keras.layers.Dense(10, activation=tf.nn.softmax) # single layer perceptron\n","])\n","\n"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"cUDyscmlEaJ4","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":1000},"outputId":"5fc8a423-c2e4-4a7b-ac32-8e20e9e9a6cc","executionInfo":{"status":"ok","timestamp":1564629807001,"user_tz":-540,"elapsed":32492,"user":{"displayName":"군고구마","photoUrl":"https://lh5.googleusercontent.com/-I0XUuvaS1h0/AAAAAAAAAAI/AAAAAAAAAC4/1a-GQoFjqY4/s64/photo.jpg","userId":"10196243389166907276"}}},"source":["model.compile(optimizer='adam', \n","              loss='sparse_categorical_crossentropy',\n","              metrics=['accuracy'])\n","\n","history = model.fit(train_images[:2000],\n","          train_labels[:2000],\n","          epochs=30,\n","          validation_data=(test_images, test_labels)\n","         )"],"execution_count":89,"outputs":[{"output_type":"stream","text":["Train on 2000 samples, validate on 10000 samples\n","Epoch 1/30\n","2000/2000 [==============================] - 2s 963us/sample - loss: 1.0603 - acc: 0.6325 - val_loss: 0.8276 - val_acc: 0.6882\n","Epoch 2/30\n","2000/2000 [==============================] - 1s 478us/sample - loss: 0.5790 - acc: 0.8010 - val_loss: 0.6356 - val_acc: 0.7744\n","Epoch 3/30\n","2000/2000 [===================